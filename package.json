{
  "name": "eweb",
  "main": "dist/eweb.js",
  "keywords": [
    "eslyz",
    "js",
    "utils",
    "css",
    "function",
    "object",
    "string",
    "array",
    "axios",
    "vue",
    "event-bus",
    "pub-sub",
    "promise"
  ],
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "webpack",
    "lint": "vue-cli-service lint",
    "doc:dev": "npx vuepress dev docs",
    "doc:build": "vuepress build docs",
    "doc:deploy": "gh-pages -d docs/dist",
    "build:watch": "webpack --watch"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "gh-pages": "^3.2.3",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.9.7"
  },
  "eslintConfig": {
    "root": false,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off",
      "no-unused-expressions": "off",
      "no-unused-labels": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
